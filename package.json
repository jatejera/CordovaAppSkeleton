    "version": "1.0.0",
    "description": "A sample Apache Cordova application that responds to the deviceready event.",
    "main": "index.html",
    "scripts": {
        "dev": "parcel index.html",
        "build": "parcel build index.html --public-url ./ --out-dir www",
        "ios": "parcel build index.html --public-url ./ --out-dir www && cordova build ios"
