    "license": "Apache-2.0",
    "dependencies": {
        "axios": "^0.18.0",
        "bootstrap": "^4.1.2",
        "cordova-ios": "^4.5.5",
        "cordova-plugin-whitelist": "^1.3.3",
        "html": "^1.0.0",
        "jquery": "^3.3.1",
        "js": "^0.1.0",
        "onsenui": "^2.10.1",
        "popper.js": "^1.14.3",
        "vue": "^2.5.16",
        "vue-hot-reload-api": "^2.3.0",
        "vue-router": "^3.0.1",
        "vue2-animate": "^2.0.0"
    },
    "devDependencies": {
        "@vue/component-compiler-utils": "^2.1.0",
        "babel-core": "^6.26.3",
        "babel-plugin-transform-html-import-to-string": "0.0.1",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "cssnano": "^4.0.3",
        "node-sass": "^4.9.2",
        "vue-template-compiler": "^2.5.16"
